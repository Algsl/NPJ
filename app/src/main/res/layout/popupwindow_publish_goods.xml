<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="@dimen/dp_271"
    android:layout_height="wrap_content"
    android:background="@drawable/white_conner_8">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_50">
    </RelativeLayout>
    <TextView
        android:id="@+id/pw_publishGoods_tv_title"
        android:layout_marginTop="@dimen/dp_17"
        android:layout_marginBottom="@dimen/dp_17"
        android:layout_alignParentTop="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:textSize="@dimen/sp_16"
        android:textColor="@color/text3"
        android:text="提示信息"/>
    <TextView
        android:layout_below="@id/pw_publishGoods_tv_title"
        android:id="@+id/pw_publishGoods_tv_content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginRight="@dimen/dp_15"
        android:textSize="@dimen/sp_14"
        android:textColor="@color/text6"
        android:lineHeight="@dimen/dp_21"/>
    <Button
        android:id="@+id/pw_publishGoods_tv_know"
        android:layout_centerInParent="true"
        android:layout_width="@dimen/dp_120"
        android:layout_height="@dimen/dp_30"
        android:textColor="@android:color/white"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="@dimen/dp_20"
        android:layout_marginBottom="@dimen/dp_20"
        android:text="我知道了"
        android:background="@drawable/theme_conner_15"/>
</RelativeLayout>